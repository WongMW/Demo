  <Global>
    <!--Note that as of e-Business 5.5.1, the default error page is defined in the web.config by the GenericErrorPageURL key. Issue 13029-->
    <!--web page where Login control is implemented for default redirects-->
    <LoginPage>Login.aspx</LoginPage>
    <ResetPasswordPage>/new-password</ResetPasswordPage>
    <!--
        true: indicates all paths should start at application's root directory and are prefixed with Request.ApplicationPath property 
        false: indicates all paths are either relative or absolute and are returned as-is
    -->
    <useVirtualPath>true</useVirtualPath>
    <!--number of incorrect login attempts before disabling account...(default value is 0 indicating unlimited tries-->
    <maxLoginTries>7</maxLoginTries>
    <!--minimum required length for a password...(default length is 7)-->
    <minPwdLength>8</minPwdLength>
    <!--minimum required number of upper-case letter in a password...(default count is 1)-->
    <minPwdUpperCase>1</minPwdUpperCase>
    <!--minimum required number of lower-case letter in a password...(default count is 1)-->
    <minPwdLowerCase>1</minPwdLowerCase>
    <!--minimum required number of number characters in a password...(default count is 1)-->
    <minPwdNumbers>1</minPwdNumbers>
    <!--Default value for System-->
    <SocialNetworkSystemName>LinkedIn</SocialNetworkSystemName>

    <!--Anil Changess for issue 12718-->
    <SocialNetworkSynchTnCText>Terms and Conditions </SocialNetworkSynchTnCText>
    <!--end-->
    <SocialNetworkSynchTnCPage>/SocialNetworkTnCPage</SocialNetworkSynchTnCPage>
